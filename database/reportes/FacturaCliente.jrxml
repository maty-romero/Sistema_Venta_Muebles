	<parameter name="NroVenta" class="java.lang.String"/>
	<parameter name="SubReportPath" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT nombre_producto AS nombre_item,unidades_vendidas_prod AS unidades, CONCAT("$",FORMAT(precio_venta_prod,2,'en_US')) AS precio_unitario,id_producto AS nro_producto,  CONCAT("$",FORMAT(unidades_vendidas_prod*precio_venta_prod, 2, 'en_US')) AS total
		FROM  producto_venta 
		LEFT JOIN productos ON productos.id = producto_venta.id_producto
		WHERE producto_venta.id_venta=$P{NroVenta} 
		
		UNION 
		
		SELECT CONCAT("Combo ", nombre_combo) AS nombre_item,unidades_vendidas_combo AS unidades , CONCAT("$",FORMAT(precio_combo,2,'en_US')) AS precio_unitario,oferta_combo_venta.id_oferta_combo AS nro_producto, CONCAT("$",FORMAT(unidades_vendidas_combo * precio_combo, 2, 'en_US')) AS total
		FROM oferta_combo_venta 
		LEFT JOIN oferta_combo ON oferta_combo.id_oferta_combo=oferta_combo_venta.id_oferta_combo
		WHERE oferta_combo_venta.id_venta=$P{NroVenta}]]>
	</queryString>
