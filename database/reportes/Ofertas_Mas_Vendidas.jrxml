	<parameter name="Fecha_inicio" class="java.lang.String"/>
	<parameter name="Fecha_fin" class="java.lang.String"/>
	<queryString language="SQL">
	<![CDATA[SELECT 
    	o.id AS id_oferta,
    	o.fecha_inicio_oferta,
    	o.fecha_fin_oferta,
    	CONCAT('Unitaria ', p.nombre_producto) AS tipo_oferta,
    	o.porcentaje_descuento,
    	SUM(pv.unidades_vendidas_prod) AS total_ventas
	FROM ofertas o
	INNER JOIN 
		producto_venta pv ON pv.id_oferta = o.id
	INNER JOIN 
		ventas v ON pv.id_venta = v.id
	INNER JOIN 
		productos p ON p.id = pv.id_producto
	INNER JOIN
		oferta_producto op ON op.id_oferta = o.id
	WHERE 
		(DATE(v.fecha_venta) BETWEEN DATE($P{Fecha_inicio}) AND DATE($P{Fecha_fin}))
	GROUP BY 
		o.id, p.nombre_producto

	UNION

	SELECT 
		o.id AS id_oferta,
		o.fecha_inicio_oferta,
		o.fecha_fin_oferta,
		CONCAT('Combo ', oc.nombre_combo) AS tipo_oferta,
		o.porcentaje_descuento,
		COUNT(*) AS total_ventas
	FROM 
		ofertas o
	JOIN 
		oferta_combo oc ON o.id = oc.id_oferta_combo
	JOIN 
		oferta_combo_venta ocv ON oc.id_oferta_combo = ocv.id_oferta_combo
	JOIN 
		ventas v ON ocv.id_venta = v.id
	WHERE 
		(DATE(v.fecha_venta) BETWEEN DATE($P{Fecha_inicio}) AND DATE($P{Fecha_fin}))
	GROUP BY 
		o.id

	UNION

	SELECT 
		o.id AS id_oferta,
		o.fecha_inicio_oferta,
		o.fecha_fin_oferta,
		CONCAT('Oferta de ', tm.nombre_tipo_mueble) AS tipo_oferta,
		o.porcentaje_descuento,
		SUM(pv.unidades_vendidas_prod) AS total_ventas
	FROM ofertas o
	INNER JOIN 
		producto_venta pv ON pv.id_oferta = o.id
	INNER JOIN 
		ventas v ON pv.id_venta = v.id
	INNER JOIN
		ofertas_tipos_muebles ot ON ot.id_oferta_tipo = o.id
	INNER JOIN
		tipos_muebles tm ON tm.id = ot.id_tipo_mueble
	WHERE 
		(DATE(v.fecha_venta) BETWEEN DATE($P{Fecha_inicio}) AND DATE($P{Fecha_fin}))
	GROUP BY o.id
	ORDER BY 
    total_ventas DESC;]]>
	</queryString>
