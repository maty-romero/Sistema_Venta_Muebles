    <parameter name="Fecha_inicio" class="java.lang.String"/>
    <parameter name="Fecha_fin" class="java.lang.String"/>
    <queryString language="SQL">
        <![CDATA[
            SELECT
                productos.nombre_producto as nombre_producto,
                COUNT(ventas.id) AS ventas_realizadas,
                productos.precio_producto AS precio_unitario,
                SUM(unidades_vendidas_prod) AS total_unidades_vendidas,
                tp.nombre_tipo_mueble as nombre_tipo_mueble
            FROM 
                producto_venta 
            LEFT JOIN 
                productos ON producto_venta.id_producto = productos.id
            LEFT JOIN 
                ventas ON producto_venta.id_venta = ventas.id
            INNER JOIN 
                tipos_muebles tp ON tp.id = productos.id_tipo_mueble
            WHERE 
                ventas.fecha_venta BETWEEN $P{Fecha_inicio} AND $P{Fecha_fin} 
            GROUP BY 
                producto_venta.id_producto
            ORDER BY 
                total_unidades_vendidas DESC;

        ]]>
    </queryString>
